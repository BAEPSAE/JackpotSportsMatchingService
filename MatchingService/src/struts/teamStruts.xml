<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />

    <package name="team" namespace="/team" extends="default, json-default">

       <action name="teamPage" class="action.TeamAction" method="teamPage">
       		<result>../teamPage.jsp</result>
       </action>
       
       <action name="t_delete"  class="action.TeamAction" method="t_delete">
       		<result type="redirect">/team/teamPage</result>
       </action>
       
       <action name="t_open"  class="action.TeamAction" method="t_open">
       		<result type="json"></result>
       </action>
       
       <action name="t_close"  class="action.TeamAction" method="t_close">
       		<result type="json"></result>
       </action>
       
       <action name="t_giveLeader"  class="action.TeamAction" method="t_giveLeader">
       		<result type="redirect">/team/teamPage</result>
       </action>
       
       
       
       
    </package>


</struts>
